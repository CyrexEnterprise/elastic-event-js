{
  "extends": "airbnb/legacy",
  "env": {
    "amd": true,
    "commonjs": true,
    "browser": true
  },
  "rules": {
    "func-names": 0,
    "strict": 0,
    // not sure why airbnb turned this on. gross!
    "comma-dangle": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
